    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@types/semver": "^7.5.6",
    "@types/update-notifier": "^6.0.8",
