BRIGHTDATA_PASSWORD=your-brightdata-password


# ============================================================
# Marketplace Settings
# ============================================================

# Platform fee percentage (0-100)
# Status: Currently configured ✓
# Example: 30
MARKETPLACE_FEE_PERCENTAGE=30

# Minimum component price in cents
# Status: Currently configured ✓
# Example: 99
MARKETPLACE_MIN_PRICE=99

# Maximum component price in cents
# Status: Not configured
# Example: 99900
MARKETPLACE_MAX_PRICE=99900

# Default marketplace currency
# Status: Currently configured ✓
# Example: USD
MARKETPLACE_CURRENCY=USD

# Days before creator payout
# Status: Not configured
# Example: 7
MARKETPLACE_PAYOUT_DELAY_DAYS=7


# ============================================================
# Experimental Features
