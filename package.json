{
  "name": "revolutionary-ui",
  "version": "3.3.1",
  "description": "Revolutionary UI v3.3.1 - Modern CLI with Best Practices from v0, Shadcn, Real Database Integration, and Multi-Framework Support",
  "keywords": [
    "ui",
    "factory",
    "components",
    "react",
    "vue",
    "angular",
    "ai",
    "gpt-4",
    "claude",
    "anthropic",
    "openai",
    "gemini",
    "ollama",
    "website-analyzer",
    "mcp",
    "interactive-cli",
    "code-generation",
    "declarative",
    "framework-agnostic",
    "revolutionary",
    "code-reduction",
    "productivity"
  ],
  "author": {
    "name": "Vladimir Dukelic",
    "email": "vladimir@dukelic.com",
    "url": "https://github.com/siliconyouth"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siliconyouth/revolutionary-ui"
  },
  "funding": {
    "type": "individual",
    "url": "https://revolutionary-ui.com/sponsor"
  },
  "homepage": "https://revolutionary-ui.com",
  "bugs": {
    "url": "https://github.com/siliconyouth/revolutionary-ui/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/frameworks/react/ReactFactory.d.ts",
      "import": "./dist/frameworks/react/ReactFactory.js",
      "require": "./dist/frameworks/react/ReactFactory.js"
    },
    "./cli": "./bin/cli.js"
  },
  "bin": {
    "revolutionary-ui": "./bin/revolutionary-ui",
    "rui": "./bin/revolutionary-ui"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:cli && npm run build:visual-builder && npm run build:terminal-ui",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:cli": "node build-cli.js",
    "build:visual-builder": "tsc -p tsconfig.build.visual-builder.json",
    "build:terminal-ui": "tsc -p src/cli/ui/tsconfig.json",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "postinstall": "node dist/lib/factory/postinstall.js || exit 0",
    "test": "node tests/run-tests.js",
    "lint": "echo 'Linting not configured yet'",
    "cli": "tsx src/lib/factory/cli.ts",
    "cli:analyze": "tsx src/lib/factory/cli.ts analyze",
    "cli:setup": "tsx src/lib/factory/cli.ts setup",
    "terminal-ui:demo": "node dist/cli/ui/demo.js",
    "terminal-ui:dev": "tsx src/cli/ui/demo.ts",
    "test:ai": "tsx src/cli/test-ai-providers.ts",
    "test:firecrawl": "tsx test-firecrawl-api.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "library": "tsx src/component-library/cli/ComponentLibraryCLI.ts",
    "library:search": "tsx src/component-library/cli/ComponentLibraryCLI.ts search",
    "prisma:generate": "dotenvx run -f .env.local -- prisma generate",
    "prisma:migrate": "dotenvx run -f .env.local -- prisma migrate dev",
    "prisma:push": "dotenvx run -f .env.local -- prisma db push",
    "prisma:studio": "dotenvx run -f .env.local -- prisma studio",
    "prisma:seed": "dotenvx run -f .env.local -- tsx prisma/seed.ts",
    "seed:billing-test": "node -r dotenv/config scripts/test-billing-page.js dotenv_config_path=.env.local",
    "db:setup": "npm run prisma:generate && npm run prisma:push",
    "db:reset": "prisma migrate reset",
    "migrate:r2": "dotenvx run -f .env.local -- tsx scripts/migrate-to-r2.ts",
    "check:r2": "dotenvx run -f .env.local -- tsx scripts/check-r2-migration-status.ts",
    "db:import-all": "dotenvx run -f .env.local -- tsx scripts/import-all-configurations.ts",
    "env:check": "node scripts/check-env.js",
    "env:load": "dotenvx run -f .env.local -- node -e \"console.log('Environment loaded successfully')\"",
    "test:r2-api": "dotenvx run -f .env.local -- tsx scripts/test-r2-api.ts",
    "backup:r2": "dotenvx run -f .env.local -- tsx scripts/backup-r2-to-s3.ts",
    "configure:r2-cors": "dotenvx run -f .env.local -- tsx scripts/configure-r2-cors.ts",
    "configure:r2-caching": "dotenvx run -f .env.local -- tsx scripts/configure-r2-caching.ts",
    "purge-cache": "dotenvx run -f .env.local -- tsx scripts/purge-r2-cache.ts",
    "setup:vector-search": "dotenvx run -f .env.local -- tsx scripts/setup-vector-search.ts",
    "embeddings:generate": "dotenvx run -f .env.local -- tsx scripts/generate-embeddings.ts",
    "embeddings:generate:dry": "npm run embeddings:generate -- --dry-run",
    "embeddings:generate:alt": "dotenvx run -f .env.local -- tsx scripts/generate-embeddings-alt.ts",
    "embeddings:generate:local": "dotenvx run -f .env.local -- tsx scripts/generate-local-embeddings.ts",
    "search:test": "dotenvx run -f .env.local -- tsx scripts/test-semantic-search.ts"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@dotenvx/dotenvx": "^1.48.4",
    "@google/generative-ai": "^0.24.1",
    "@mistralai/mistralai": "^1.7.5",
    "@modelcontextprotocol/sdk": "^1.17.1",
    "@prisma/client": "^6.13.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/glob": "^9.0.0",
    "@types/prismjs": "^1.26.5",
    "@xenova/transformers": "^2.17.2",
    "algoliasearch": "^5.35.0",
    "axios": "^1.11.0",
    "chalk": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "cli-table3": "^0.6.5",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "glob": "^11.0.3",
    "groq-sdk": "^0.29.0",
    "inquirer": "^12.9.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.11.0",
    "ora": "^8.2.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prismjs": "^1.30.0",
    "puppeteer": "^24.15.0",
    "semver": "^7.7.2",
    "stripe": "^18.4.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "codemirror": "^6.0.2",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.536.0",
